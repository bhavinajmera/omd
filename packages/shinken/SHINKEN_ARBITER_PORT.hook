#!/bin/bash

# Alias: TCP port the Shinken arbiter listens on
# Menu: Basic
# Description:
#  If you configured Shinken as the monitoring core, this is the
#  listening port of the arbiter .

case "$1" in
    default)
        echo "localhost:7770"
    ;;
    choices)
        echo "(none)|([1-9][0-9]{0,4})|([a-zA-Z0-9\.\-]*:[1-9][0-9]{0,4})"
    ;;
    set)
    ;;
    depends)
    [ "$CONFIG_CORE" = shinken ]
    ;;
esac
